# NurdsCode Docker Stack Environment
# Copy to .env and fill in your values

# ===========================================
# DATABASE
# ===========================================
POSTGRES_USER=nurdscode
POSTGRES_PASSWORD=your-secure-password
POSTGRES_DB=nurdscode_dev
POSTGRES_PORT=5432

# ===========================================
# REDIS
# ===========================================
REDIS_PORT=6379

# ===========================================
# LLM PROVIDERS
# ===========================================
# At least one LLM provider is required

# Anthropic (Claude)
ANTHROPIC_API_KEY=sk-ant-your-key

# OpenAI
OPENAI_API_KEY=sk-your-key

# Google AI (Gemini)
GOOGLE_AI_API_KEY=your-gemini-key

# LLM Configuration (JSON format)
LLM_CONFIGS={"default":{"model":"claude-3-5-sonnet-20241022","api_key":"${ANTHROPIC_API_KEY}","provider":"anthropic"}}

# ===========================================
# SEARCH PROVIDERS
# ===========================================
TAVILY_API_KEY=tvly-your-key
SERP_API_KEY=
FIRECRAWL_API_KEY=

# ===========================================
# SUPABASE
# ===========================================
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key

# ===========================================
# GCP CONFIGURATION
# ===========================================
GCP_PROJECT_ID=your-project-id
GOOGLE_APPLICATION_CREDENTIALS=/app/credentials/gcp-service-account.json

# Storage buckets
STORAGE_CONFIG__GCS_BUCKET_NAME=your-bucket
STORAGE_CONFIG__GCS_PROJECT_ID=your-project-id

# ===========================================
# SANDBOX (E2B)
# ===========================================
E2B_API_KEY=your-e2b-key
SANDBOX_TEMPLATE_ID=m4zta9txnip2o1xq6i8u

# ===========================================
# NETWORKING
# ===========================================
NGROK_AUTHTOKEN=your-ngrok-token
NGROK_REGION=us

# ===========================================
# PORTS
# ===========================================
BACKEND_PORT=8000
FRONTEND_PORT=80
SANDBOX_SERVER_PORT=8100
TOOL_SERVER_PORT=1236
NGROK_METRICS_PORT=4040

# ===========================================
# II-AGENT RESEARCH CONFIG
# ===========================================
RESEARCHER_AGENT_CONFIG={"researcher":{},"report_builder":{},"final_report_builder":{}}
