{
  "name": "nurds_code_dot_com",
  "version": "1.0.0",
  "description": "ACHIEVEMOR CODING PLATFORM",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "worker:dev": "wrangler dev workers/api.js",
    "worker:deploy": "wrangler deploy workers/api.js",
    "worker:deploy:staging": "wrangler deploy workers/api.js --env staging",
    "worker:deploy:prod": "wrangler deploy workers/api.js --env production",
    "db:migrate": "wrangler d1 execute nurdscode_db --file=./schema.sql",
    "supabase:migrate": "psql $SUPABASE_URL -f supabase_schema.sql",
    "setup:secrets": "pwsh -File scripts/setup-secrets.ps1",
    "setup:storage": "pwsh -File scripts/setup-cloudflare-storage.ps1",
    "supabase:migrate:ps": "pwsh -File scripts/apply-supabase-schema.ps1",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "deploy:full": "npm run build && npm run worker:deploy:prod",
    "test": "echo 'Tests not yet implemented' && exit 0",
    "test:algorand": "node scripts/test-algorand-connection.js",
    "test:phase1": "node scripts/test-phase1.js"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "react",
    "vite",
    "tailwind",
    "stripe"
  ],
  "author": "Nurdscode",
  "license": "MIT",
  "dependencies": {
    "@clerk/backend": "1.13.0",
    "@clerk/clerk-react": "5.53.4",
    "@clerk/types": "4.20.0",
    "@cloudflare/ai": "^1.2.2",
    "@microsoft/teams-js": "^2.46.0",
    "@monaco-editor/react": "^4.7.0",
    "@perawallet/connect": "^1.4.2",
    "@stripe/stripe-js": "^8.2.0",
    "@supabase/supabase-js": "^2.78.0",
    "@zoom/videosdk": "^2.3.0",
    "algosdk": "^3.5.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.548.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.5",
    "simple-git": "^3.30.0",
    "stripe": "^19.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251014.0",
    "@tailwindcss/postcss": "^4.1.16",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.16",
    "vite": "^7.1.12",
    "wrangler": "^4.45.3"
  }
}
